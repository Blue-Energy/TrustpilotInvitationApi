{
    "name": "itspirit/trustpilot-invitation-api",
    "description": "A PHP library for accessing the Trustpilot Invitation API",
    "license": "MIT",
    "type": "project",
    "keywords": [
      "trustpilot",
      "review",
      "invitation",
      "api"
    ],
    "require": {
        "php": ">=5.6.0",
        "guzzlehttp/guzzle": "~6.0",
        "moneymaxim/trustpilot-authenticator": "^0.1.1"
    },
    "authors": [
        {
          "name": "Michael Frank",
          "email": "contact@phpdoctor.cc",
          "homepage": "https://www.the-Spirit-of-IT.com/"
        },
        {
            "name": "Andrew Carter",
            "email": "andrew.carter@moneymaxim.co.uk"
        }
    ],
    "autoload": {
        "psr-4": {
            "Trustpilot\\Api\\Invitation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Trustpilot\\Test\\Api\\Invitation\\": "test/"
        }
    },
  "config": {
    "platform": {
      "php": "5.6.11"
    },
    "sort-packages": true,
    "optimize-autoloader": true
  }
}

